//biblioteca de icones
yarn add react-icons

//consumir api
yarn add axios

//Auxiliar o get de prop
yarn add prop-types

//Adição da dependencia do reactotron com o reactotron redux
yarn add reactotron-react-js reactotron-redux

//'Immer' para tratar arrays
yarn add immer

yarn add redux-sage

//Adicionando plugin do redux-sagaa
yarn add reactotron-redux-saga

//mensagens de erro
yarn add react-toastify

//ajuda nas rotas
yarn add history
